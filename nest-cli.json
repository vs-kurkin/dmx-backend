{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "typeCheck": true,
    "builder": "tsc",
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "entryFile": "../src/main.ts",
  "generateOptions": {
    "baseDir": "dist"
  },
  "language": "ts",
  "sourceRoot": "src"
}
